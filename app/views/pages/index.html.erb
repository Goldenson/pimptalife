<div class="parallax-container">
  <div class="parallax"><%= image_tag('life.jpg') %></div>
</div>

<div class="container">
	<div class="row">
      <div class="col s12">
        <div class="card-panel z-depth-2 m-t-40">
          <span class="flow-text">
            <p>Share your Pimps with everyone. <a class="waves-effect waves-teal btn-flat right">Let's go <i class="mdi-content-send right"></i></a></p>
          </span>
        </div>
      </div>
      <div class="col s12">
        <h1 class="center-align"><b>Latest Pimps updated</b></h1>
        <div class="row">
          <div class="col s12 m6 l4">
            <div class="card z-depth-4">
              <div class="card-image waves-effect waves-block waves-light">
                <%= image_tag('life.jpg') %>
              </div>
              <div class="card-content">
                <span class="card-title grey-text text-darken-4">Title</span>
                <span class="card-title grey-text text-darken-4"><i class="fa fa-heart-o right"> 3</i></span>
                <p><i class="mdi-image-timer-auto"></i>User</p>
                <p><small><i class="mdi-device-access-time m-r-10"></i>Date<i class="small mdi-content-clear right"></i></small></p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
</div>

<script type="text/javascript">
	$(document).ready(function(){
      $('.parallax').parallax();
    });
</script>